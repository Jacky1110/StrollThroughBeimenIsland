<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.StrollThroughBeimenIsland" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/c_4b525b_100</item>
        <item name="colorPrimaryVariant">@color/c_4b525b_100</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.StrollThroughBeimenIsland.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.StrollThroughBeimenIsland.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.StrollThroughBeimenIsland.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="store_line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">17dp</item>
        <item name="android:paddingStart">10dp</item>

    </style>

    <style name="DatePickerTheme" parent="android:Theme.Holo.Light.Dialog.MinWidth">
        <item name="android:windowBackground">@android:color/transparent</item>

    </style>
</resources>